    "import os\n",
    "import cv2\n",
    "from scipy import ndimage"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f33c2a27-72f0-4aa3-b00b-af00bd9e8c9b",
   "metadata": {},
   "outputs": [],
   "source": [
    "PATH = 'Dataset/'\n",
    "RESULTS = 'Results/'"
   ]
  },
  {
   "cell_type": "code",
