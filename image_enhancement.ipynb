   "source": [
    "from skimage.metrics import structural_similarity as ssim\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def evaluate_image_quality(original_img_path, enhanced_img_path):\n",
    "    \"\"\"\n",
    "    Evaluates the quality of an enhanced image compared to the original.\n",
    "\n",
    "    Args:\n",
    "        original_img_path (str): The path to the original image.\n",
    "        enhanced_img_path (str): The path to the enhanced image.\n",
    "\n",
    "    Returns:\n",
    "        dict: A dictionary containing evaluation metrics.\n",
    "    \"\"\"\n",
    "    try:\n",
    "        original_img = cv2.imread(original_img_path, cv2.IMREAD_GRAYSCALE)\n",
    "        enhanced_img = cv2.imread(enhanced_img_path, cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "        if original_img is None:\n",
    "            print(f\"Error: Original image not found at {original_img_path}\")\n",
    "            return None\n",
    "        if enhanced_img is None:\n",
    "             print(f\"Error: Enhanced image not found at {enhanced_img_path}\")\n",
    "             return None\n",
    "\n",
    "        # Resize enhanced image to match original if dimensions differ\n",
    "        if original_img.shape != enhanced_img.shape:\n",
    "            print(f\"Warning: Enhanced image shape {enhanced_img.shape} does not match original shape {original_img.shape}. Resizing enhanced image.\")\n",
    "            enhanced_img = cv2.resize(enhanced_img, (original_img.shape[1], original_img.shape[0]))\n",
    "\n",
    "\n",
    "        # Calculate Mean Squared Error (MSE)\n",
    "        mse = np.mean((original_img - enhanced_img) ** 2)\n",
    "\n",
    "        # Calculate Peak Signal-to-Noise Ratio (PSNR)\n",
    "        if mse == 0:\n",
    "            psnr = float('inf') # Images are identical\n",
    "        else:\n",
    "            max_pixel_value = 255.0\n",
    "            psnr = 20 * np.log10(max_pixel_value / np.sqrt(mse))\n",
    "\n",
    "        # Calculate Structural Similarity Index (SSIM)\n",
    "        # Data range for grayscale images is 255\n",
    "        ssim_index = ssim(original_img, enhanced_img, data_range=255)\n",
    "\n",
    "        return {\n",
    "            'MSE': mse,\n",
    "            'PSNR': psnr,\n",
    "            'SSIM': ssim_index\n",
    "        }\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"Error during quality evaluation: {e}\")\n",
    "        return None\n",
    "\n",
    "def analyze_results(original_dir, enhanced_dir):\n",
    "    \"\"\"\n",
    "    Analyzes the results of image enhancement for a directory of images.\n",
    "\n",
    "    Args:\n",
    "        original_dir (str): The path to the directory containing original images.\n",
    "        enhanced_dir (str): The path to the directory containing enhanced images.\n",
    "    \"\"\"\n",
    "    print(\"\\n--------PROJECT EVALUATION----------\")\n",
    "    print(\"------------------------------------\")\n",
    "    print(\"--------ANALYZING IMAGE QUALITY-----\")\n",
    "\n",
    "    evaluation_metrics = {}\n",
    "    processed_count = 0\n",
    "\n",
    "    for filename in os.listdir(original_dir):\n",
    "        original_img_path = os.path.join(original_dir, filename)\n",
    "        # Assuming enhanced files are named 'enhanced_[original_filename]'\n",
    "        enhanced_filename = f\"enhanced_{filename}\"\n",
    "        enhanced_img_path = os.path.join(enhanced_dir, enhanced_filename)\n",
    "\n",
    "        # Check if both original and enhanced files exist and are images\n",
    "        if os.path.isfile(original_img_path) and os.path.isfile(enhanced_img_path) and \\\n",
    "           filename.lower().endswith(('.png', '.jpg', '.jpeg')):\n",
    "\n",
    "            print(f\"Evaluating {filename}...\")\n",
    "            metrics = evaluate_image_quality(original_img_path, enhanced_img_path)\n",
    "\n",
    "            if metrics:\n",
    "                evaluation_metrics[filename] = metrics\n",
    "                processed_count += 1\n",
    "            else:\n",
    "                print(f\"Could not evaluate {filename}.\")\n",
    "\n",
    "    if processed_count > 0:\n",
    "        print(\"\\n--- Summary of Evaluation Metrics ---\")\n",
    "        average_mse = np.mean([m['MSE'] for m in evaluation_metrics.values()])\n",
    "        average_psnr = np.mean([m['PSNR'] for m in evaluation_metrics.values() if m['PSNR'] != float('inf')]) # Exclude inf for averaging\n",
    "        average_ssim = np.mean([m['SSIM'] for m in evaluation_metrics.values()])\n",
    "\n",
    "        print(f\"Processed {processed_count} image pairs.\")\n",
    "        print(f\"Average MSE: {average_mse:.4f}\")\n",
    "        print(f\"Average PSNR: {average_psnr:.4f}\")\n",
    "        print(f\"Average SSIM: {average_ssim:.4f}\")\n",
    "\n",
    "        # Optional: Plot metrics\n",
    "        # plot_metrics(evaluation_metrics)\n",
    "\n",
    "    else:\n",
    "        print(\"No image pairs found for evaluation.\")\n",
    "\n",
    "    print(\"--------IMAGE EVALUATION COMPLETED------\")\n",
    "\n",
    "def plot_metrics(metrics_dict):\n",
    "    \"\"\"\n",
    "    Plots the evaluation metrics for each image.\n",
    "\n",
    "    Args:\n",
    "        metrics_dict (dict): A dictionary where keys are filenames and values are metric dictionaries.\n",
    "    \"\"\"\n",
    "    filenames = list(metrics_dict.keys())\n",
    "    mse_values = [metrics['MSE'] for metrics in metrics_dict.values()]\n",
    "    psnr_values = [metrics['PSNR'] for metrics in metrics_dict.values()]\n",
    "    ssim_values = [metrics['SSIM'] for metrics in metrics_dict.values()]\n",
    "\n",
    "    x = np.arange(len(filenames))\n",
    "    width = 0.2\n",
    "\n",
    "    fig, ax1 = plt.subplots(figsize=(15, 6))\n",
    "\n",
    "    rects1 = ax1.bar(x - width, mse_values, width, label='MSE', color='skyblue')\n",
    "    ax1.set_ylabel('MSE', color='skyblue')\n",
    "    ax1.tick_params(axis='y', labelcolor='skyblue')\n",
    "\n",
    "    ax2 = ax1.twinx()\n",
    "    rects2 = ax2.bar(x, psnr_values, width, label='PSNR', color='lightcoral')\n",
    "    ax2.set_ylabel('PSNR', color='lightcoral')\n",
    "    ax2.tick_params(axis='y', labelcolor='lightcoral')\n",
    "\n",
    "    ax3 = ax1.twinx()\n",
    "    ax3.spines['right'].set_position(('outward', 60)) # Offset the third axis\n",
    "    rects3 = ax3.bar(x + width, ssim_values, width, label='SSIM', color='lightgreen')\n",
    "    ax3.set_ylabel('SSIM', color='lightgreen')\n",
    "    ax3.tick_params(axis='y', labelcolor='lightgreen')\n",
    "\n",
    "\n",
    "    ax1.set_xlabel('Image Filename')\n",
    "    ax1.set_title('Image Quality Evaluation Metrics by Image')\n",
    "    ax1.set_xticks(x)\n",
    "    ax1.set_xticklabels(filenames, rotation=45, ha='right')\n",
    "    ax1.legend(loc='upper left')\n",
    "    ax2.legend(loc='upper right')\n",
    "    ax3.legend(loc='lower right')\n",
    "\n",
    "\n",
    "    fig.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "\n",
    "# Call the analysis function after enhancing images\n",
    "analyze_results(original_dir=PATH, enhanced_dir=RESULTS)"
   ]
  }
 ],
